1
normal! 0
wincmd w
3wincmd w
exe 'vert 1resize ' . ((&columns * 1 + 61) / 123)
exe '2resize ' . ((&lines * 2 + 18) / 36)
exe 'vert 2resize ' . ((&columns * 121 + 61) / 123)
exe '3resize ' . ((&lines * 30 + 18) / 36)
exe 'vert 3resize ' . ((&columns * 113 + 61) / 123)
exe '4resize ' . ((&lines * 30 + 18) / 36)
exe 'vert 4resize ' . ((&columns * 1 + 61) / 123)
exe '5resize ' . ((&lines * 30 + 18) / 36)
exe 'vert 5resize ' . ((&columns * 1 + 61) / 123)
exe '6resize ' . ((&lines * 30 + 18) / 36)
exe 'vert 6resize ' . ((&columns * 1 + 61) / 123)
exe '7resize ' . ((&lines * 30 + 18) / 36)
exe 'vert 7resize ' . ((&columns * 1 + 61) / 123)
tabnext 1
if exists('s:wipebuf')
  silent exe 'bwipe ' . s:wipebuf
endif
