let s:l = 1532 - ((14 * winheight(0) + 15) / 30)
if s:l < 1 | let s:l = 1 | endif
exe s:l
normal! zt
1532
normal! 0
wincmd w
argglobal
edit main.cpp
setlocal keymap=
